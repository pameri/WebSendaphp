                </div><!-- /detail -->



              </div><!-- /details -->

            </div><!-- /details_wrapper -->

            <div class="paging">
              <div id="numbers"></div>
              <a href="javascript:void(0);" class="previous" title="Previous">Previous</a>
              <a href="javascript:void(0);" class="next" title="Next">Next</a>
            </div><!-- /paging -->

          </div><!-- /panel -->

          <div class="backgrounds">
